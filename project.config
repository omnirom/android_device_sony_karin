[access]
	inheritFrom = All-Projects
[access "refs/heads/*"]
	create = group Sony Common Maintainers
	create = group Sony Karin Maintainers
	create = group Sony Kitakami Maintainers
	push = group Sony Common Maintainers
	push = +force group Sony Karin Maintainers
	push = group Sony Kitakami Maintainers
	label-Code-Review = -2..+2 group Sony Common Maintainers
	label-Code-Review = -2..+2 group Sony Karin Maintainers
	label-Code-Review = -2..+2 group Sony Kitakami Maintainers
	label-Verified = -1..+1 group Sony Common Maintainers
	label-Verified = -1..+1 group Sony Karin Maintainers
	label-Verified = -1..+1 group Sony Kitakami Maintainers
	submit = group Sony Common Maintainers
	submit = group Sony Karin Maintainers
	submit = group Sony Kitakami Maintainers
	pushMerge = group Sony Common Maintainers
	pushMerge = group Sony Karin Maintainers
	pushMerge = group Sony Kitakami Maintainers
